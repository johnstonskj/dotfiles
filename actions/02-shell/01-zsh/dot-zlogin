# -*- mode: sh; eval: (sh-set-shell "zsh") -*-

############################################################################
# Basic Environment
############################################################################

export LC_ALL=en_US.UTF-8

export TERM="xterm-256color"

if [[ -f ${XDG_CONFIG_HOME:-~/.config}/user-dirs.dirs ]]; then
    source ${XDG_CONFIG_HOME:-~/.config}/user-dirs.dirs
fi
